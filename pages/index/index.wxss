/**index.wxss**/

page {
  background-color: #eee;
}

swiper {
  height: 400rpx;
}

.swiper image {
  width: 100%;
  height: 400rpx;
}

.weui-flex__item {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 1rpx;
  border: 1px #eee solid;
  background-color: #fff;
  height: 200rpx;

}

.weui-flex__item__text {
  display: flex;
  flex-direction: column;
  margin-right: 60rpx;
}
.buss-serv{
  font-size: 28rpx;
  color: #666666;
}
.buss-serv__desc{
  font-size: 22rpx;
  color: #999999;
}
.adv1{
  margin-top: 10rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 226rpx;
  font-size: 32rpx;
  color: #3a82f8;
  font-weight: 1000;
  text-align: center;
}
.adv1{
  width: 100%;
  height: 226rpx;
  align-items: center;
  vertical-align: middle;
}
.h_text{
  padding-top: 50rpx;
}
.l_text{
  font-size: 22rpx;
  color: #999;
  font-weight: 400;
}
.four{
  margin: 0;
  border: 0;
  display: flex;
  flex-direction: column;
  width: 50%;
}
.vlog{
  margin-top: 4rpx;
}
.four image{
  width: 100%;
}
.adv2{
  margin-top: 2rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 226rpx;
  font-size: 32rpx;
  color: #3a82f8;
  font-weight: 1000;
  text-align: center;
  width: 100%;
  height: 226rpx;
  align-items: center;
  vertical-align: middle;
  margin-top: 10rpx;
}
button{
  margin-top: 90rpx;
}
button[type=default]{
  color:#3a82f8;
}
.addr{
  width: 100%;
  height: 300rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  text-align: center;
  vertical-align: middle;
}
.addr .buss-serv{
  padding-top: 100rpx;
  padding-bottom: 20rpx;
}
.weui-footer{
  height: 80rpx;
  padding-top: 50rpx;
  background: #eee;
}