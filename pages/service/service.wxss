.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result .weui-cell__bd{
  padding: 2px 0 2px 20px;
  color: #666;
}
.searchbar-result:before{
    display: none;
}
.weui-search-bar__label{
  background-color: #EDEDED;
}
.weui-search-bar__form{
  background: #EDEDED;
}
.weui-search-bar{
  border-radius: 40rpx;
  width: 90%;
  margin: 0 auto;
  padding: 10rpx;
}
.content{
  height: 600rpx;
  background: #fff;
  margin: 0 auto;
  padding: 0 50rpx;
  margin-top: 50rpx;
  border-bottom: 10rpx #EDEDED solid;
}
.content image{
  width: 100%;
  height: 220rpx;
}
.content text{
  font-size: 26rpx;
  color: #666;
}
.adv2{
  background-image: url('data:image/png;base64,');
  background-size: 100% 226rpx;
  font-size: 32rpx;
  color: #3a82f8;
  font-weight: 1000;
  text-align: center;
  width: 100%;
  height: 226rpx;
  align-items: center;
  vertical-align: middle;
}
button{
  margin-top: 90rpx;
}
button[type=default]{
  color:#3a82f8;
}
.weui-footer{
  height: 80rpx;
  padding-top: 50rpx;
  background: #eee;
}